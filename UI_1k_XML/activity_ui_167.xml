			<CheckBox
				android:id="@+id/DSFqH"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_gravity="center"
				android:layout_weight= "2"
				android:checked="false" />
		</LinearLayout>
	</LinearLayout>
	<DatePicker
		android:id="@+id/IYMeY"
		android:layout_width="match_parent"
