			android:id="@+id/KyMJL"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_gravity="center"
			android:layout_weight= "2"
			android:text= "AetOH"
			android:textSize="26sp"
			android:textColor="#000000" />
		<EditText
			android:id="@+id/xypEv"
