			android:text= "WZuGD"
			android:textSize="16sp"
			android:textColor="#666666"
			android:background="@drawable/button_sharp" />
		<Button
			android:id="@+id/IEyfL"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_gravity="center"
			android:layout_weight= "1"
