				android:textSize="16sp"
				android:textColor="#000000" />
			<TextView
				android:id="@+id/ijKqU"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_gravity="center"
				android:layout_weight= "1"
				android:text= "wPEQQ"
				android:textSize="16sp"
				android:textColor="#000000" />
			<TextView
				android:id="@+id/RfwZE"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_gravity="center"
				android:layout_weight= "1"
				android:text= "wVMbh"
				android:textSize="16sp"
				android:textColor="#000000" />
