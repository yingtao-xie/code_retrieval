				android:layout_height="wrap_content"
				android:layout_gravity="center"
				android:layout_weight= "1"
				android:text= "efXDd"
				android:textSize="26sp"
				android:textColor="#666666"
				android:background="@drawable/button_sharp" />
			<Button
				android:id="@+id/hjofN"
