	<SeekBar
		android:id="@+id/Terqv"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_gravity="center" />
	<EditText
		android:id="@+id/ZKPDz"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_gravity="center"
		android:text= "fjtPJ"
